{
  "extends": [
    "prettier",
    "plugin:vue/strongly-recommended"
  ],
  "plugins": [
    "prettier",
    "vue"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "no-invalid-this": 0,
    "require-jsdoc": 0
  }
}
